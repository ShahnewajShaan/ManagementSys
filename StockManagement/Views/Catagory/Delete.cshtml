
@model Catagory
<form method="post" asp-action="DeletePOST">
	<div class="border p-3 mt-4">
		<div class="row pb-2">
			<h2 class="text-primary">Delete Catagory</h2>
			<hr />
		</div>
		@*<div asp-validation-summary="All"></div>*@
		<div class="mb-3">
			<label asp-for= "Name"> </label>
			<input asp-for="Name" disabled class="from-control"  />
		
		</div>
		<div class="mb-3">
			<label asp-for="DisplayOrder"> </label>
			<input asp-for="DisplayOrder" class="from-control"  />
		
		</div>
		<button type="submit" class="btn btn-primary" style="width:150px">Delete</button>
		<a asp-controller="Catagory" asp-action="Index" class="btn btn-secondary" style="width:150px">
			Back
		</a>
	</div>

</form>

@section Scripts{
@{
<partial name="_ValidationScriptsPartial" />
}
}


